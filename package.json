{
  "name": "es6-init",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "mkdir dist; npm run build",
    "clean": "rm -rf dist",
    "build": "browserify src/index.js -o dist/bundle.js",
    "build:w": "watchify src/index.js -o dist/bundle.js",
    "build:china": "babel src/2017-02-22-china/script.src.js -o src/2017-02-22-china/script.js",
    "build:china:w": "babel src/2017-02-22-china/script.src.js  --watch -o src/2017-02-22-china/script.js --source-maps inline",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix"
  },
  "keywords": [],
  "author": "Nobita",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "watchify": "^3.6.1"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-2"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "latest",
            "stage-2"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "d3v4": "^4.2.2",
    "eslint": "^3.16.1",
    "eslint-plugin-import": "^2.2.0"
  }
}
