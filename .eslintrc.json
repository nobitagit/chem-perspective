{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "d3": true
  },
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": false
    },
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "no-const-assign": "error",
    "no-this-before-super": "error",
    "no-undef": "error",
    "no-unreachable": "error",
    "no-unused-vars": "error",
    "constructor-super": "error",
    "quotes": ["error", "single"],
    "valid-typeof": "warn"
    }
}